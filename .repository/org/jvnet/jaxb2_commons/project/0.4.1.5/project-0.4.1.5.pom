<project xsi:schemaLocation='http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd' xmlns='http://maven.apache.org/POM/4.0.0' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.jvnet.jaxb2_commons</groupId>
	<artifactId>project</artifactId>
	<packaging>pom</packaging>
	<version>0.4.1.5</version>
	<name>JAXB2 Commons</name>
	<url>https://jaxb2-commons.dev.java.net/</url>
	<description>JAXB2 Commons include tools and plugins for the reference implementation of JAXB2</description>
	<issueManagement>
		<system>CollabNet</system>
		<url>https://jaxb2-commons.dev.java.net/servlets/ProjectIssues</url>
	</issueManagement>
	<inceptionYear>2005</inceptionYear>
	<mailingLists>
		<mailingList>
			<name>Users List</name>
			<subscribe>users-subscribe@jaxb.dev.java.net</subscribe>
			<unsubscribe>users-unsubscribe@jaxb2.dev.java.net</unsubscribe>
			<post>users@jaxb.dev.java.net</post>
			<archive>https://jaxb.dev.java.net/servlets/SummarizeList?listName=users</archive>
		</mailingList>
	</mailingLists>
	<developers>
		<developer>
			<id>kohsuke</id>
			<name>Kohsuke Kawaguchi</name>
			<email>kohsuke.kawaguchi@sun.com</email>
		</developer>
		<developer>
			<id>lexi</id>
			<name>Aleksei Valikov</name>
			<email>valikov@gmx.net</email>
		</developer>
	</developers>
	<contributors>
		<!--contributor>
			<name></name>
